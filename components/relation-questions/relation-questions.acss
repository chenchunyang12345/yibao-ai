.rq-wrap {
  width: 690rpx;
  padding: 0 30rpx;
  margin-bottom: 30rpx;
}

.rq-content {
  background-color: #fff;
  border-radius: 0 25rpx 25rpx 25rpx;
  padding: 22rpx;
  background-image: url('data:image/png;base64,');
  background-size: 98rpx 98rpx;
  background-position: right bottom;
  background-repeat: no-repeat;
}

.rq-title {
  margin-bottom: 20rpx;
}

.rq-title text {
  color: #666;
  font-size: 28rpx;
  line-height: 34rpx;
}

.rq-q {
  color: #4C95FF;
  font-size: 24rpx;
  line-height: 34rpx;
  display: flex;
  align-items: center;
  margin-bottom: 15rpx;
}

.rq-q::before {
  content: '';
  width: 10rpx;
  height: 10rpx;
  border-radius: 50%;
  background-color: rgba(76,149,255,0.3);
  opacity: 0.8;
  margin-right: 20rpx;
}