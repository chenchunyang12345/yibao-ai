.iq-wrap {
  width: 100%;
  height: 108rpx;
  background-color: #fff;
  border-top: 2rpx solid #DAD9E2;
  position: fixed;
  bottom: 0;
  left: 0;
  display: flex;
  align-items: center;
  justify-content: center;
}

.iq-wrap input {
  width: 614rpx;
  height: 69rpx;
  background-color: #F5F5F5;
  border-radius: 34rpx;
  margin-right: 20rpx;
}

.iq-wrap view {
  height: 69rpx;
  color: #4C95FF;
  font-size: 30rpx;
  line-height: 69rpx;
}