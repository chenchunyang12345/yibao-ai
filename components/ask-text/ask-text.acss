.at-wrap {
  width: 690rpx;
  padding: 0 30rpx;
  display: flex;
  justify-content: flex-end;
  margin-bottom: 30rpx;
}

.at-wrap .at-text {
  max-width: 590rpx;
  padding: 15rpx 25rpx 14rpx 21rpx;
  background-color: rgba(76, 149, 255, 0.3);
  box-shadow: 0px 2px 10px 0px rgba(153,153,153,0.2);
  border-radius: 25rpx 0 25rpx 25rpx;
  margin-right: 25rpx;
}

.at-wrap .at-text text {
  font-size: 30rpx;
  line-height: 45rpx;
}

.at-img {
  width: 75rpx;
  height: 75rpx;
  background-image:url('data:image/svg+xml;base64,');
  background-size: 100% 100%;
}