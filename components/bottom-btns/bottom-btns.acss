.bb-wrap {
  width: 100%;
  height: 66rpx;
  position: fixed;
  left: 0;
  bottom: 128rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 28rpx;
}

.bb-btn {
  min-width: 76rpx;
  padding: 22rpx 18rpx;
  border-radius: 33rpx;
  background-color: #fff;
  box-shadow: 0px 2px 10px 0px rgba(153,153,153,0.2);
  text-align: center;
}